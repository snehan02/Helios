{
    "version": 2,
    "builds": [
        {
            "src": "src/index.ts",
            "use": "@vercel/node",
            "config": {
                "maxDuration": 60,
                "memory": 1024
            }
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "src/index.ts"
        }
    ]
}
